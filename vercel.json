{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
  "rewrites": [
    {
      "source": "/my-apps/(.*)",
      "destination": "/$1"
    }
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
